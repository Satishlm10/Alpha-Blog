<h1>List of All the Articles</h1>
<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Description</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        <% @all_articles.each do |article|%>
        <tr>
            <td><%= article.title %></td>
            <td><%= article.description %></td>
            <td>Some Actions</td>
        </tr>
        <% end %>
    </tbody>
</table>